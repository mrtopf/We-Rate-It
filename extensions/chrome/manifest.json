{
  "name": "We Rate It",
  "version": "0.0.3",
  "description": "We Rate It ist ein Experiment zur Altersbewertung von Websites durch die Community. ",
  "background_page": "background.html",
  "update_url": "http://werateit.de/extensions/chrome/update.xml",
  "icons" : {
    "16" : "icons/icon16.png",
    "48" : "icons/icon48.png",
    "128" : "icons/icon128.png"
  },
  "page_action": {
    "default_icon": "icons/no.png",
    "popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "history",
    "http://localhost/",
    "http://localhost:9991/"
  ]
}

